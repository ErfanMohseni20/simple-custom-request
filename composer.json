{
    "name": "erfanmohseni20/simple-custom-request",
    "description": "A simple package for handling API responses and requests in Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Erfan Mohseni",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "ErfanMohseni20\\SimpleCustomRequest\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ErfanMohseni20\\SimpleCustomRequest\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
